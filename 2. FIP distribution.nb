Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    SubscriptBox["p", "CM"], " ", 
    RowBox[{"(", 
     RowBox[{"proton", "-", 
      RowBox[{"proton", " ", "collisions"}]}], ")"}], " ", "in", " ", "terms",
     " ", "of", " ", 
    SubscriptBox["p", "lab"], " ", "assuming", " ", "that", " ", 
    SubscriptBox["p", "lab"], " ", "is", " ", "aligned", " ", "along", " ", 
    "the", " ", "boost"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"PcmPlab", "[", 
      RowBox[{"plab_", ",", "m_", ",", "Eplab_", ",", "MP_"}], "]"}], "=", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"MP", ">", "0"}], "&&", 
        RowBox[{"Eplab", ">", "0"}]}], ",", 
       RowBox[{"Simplify", "[", 
        RowBox[{
         RowBox[{"\[Gamma]CMbeamDump", "[", 
          RowBox[{"Eplab", ",", "MP"}], "]"}], 
         RowBox[{"(", 
          RowBox[{"plab", "-", 
           RowBox[{
            RowBox[{"vCMbeamDump", "[", 
             RowBox[{"Eplab", ",", "MP"}], "]"}], "*", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["plab", "2"], "+", 
              SuperscriptBox["m", "2"]}]]}]}], ")"}]}], "]"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"inverse", " ", "relation"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"PlabPcm", "[", 
      RowBox[{"pcm_", ",", "m_", ",", "Eplab_", ",", "MP_"}], "]"}], "=", 
     RowBox[{
      RowBox[{"plab", "/.", 
       RowBox[{
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"PcmPlab", "[", 
            RowBox[{"plab", ",", "m", ",", "Eplab", ",", "MP"}], "]"}], 
           "\[Equal]", "pcm"}], ",", "plab"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], "//", "Simplify"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{
      "Relation", " ", "between", " ", "the", " ", "meson", " ", "and", " ", 
       "ALP", " ", "energies", " ", "and", " ", "angles", " ", "in", " ", 
       "the", " ", "CM", " ", 
       RowBox[{"frame", ".", " ", "Obtained"}], " ", "under", " ", 
       "assumption", " ", "that", " ", "at", " ", "the", " ", "CM", " ", 
       "frame", " ", "of", " ", "proton"}], "-", 
      RowBox[{
      "proton", " ", "collisions", " ", "the", " ", "meson", " ", "and", " ", 
       "ALP"}]}], " ", "|", "momenta", "|", " ", 
     RowBox[{"are", " ", "the", " ", "same"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"ECMmesonECMALP", "[", 
      RowBox[{"Ea_", ",", "ma_", ",", "mmeson_"}], "]"}], "=", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["Ea", "2"], "-", 
       SuperscriptBox["ma", "2"], "+", 
       SuperscriptBox["mmeson", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{
      "Relation", " ", "between", " ", "meson", " ", "and", " ", "ALP", " ", 
       "kinematics", " ", "in", " ", "lab", " ", 
       RowBox[{"frame", ".", " ", "Obtained"}], " ", "under", " ", 
       "assumption", " ", "that", " ", "at", " ", "the", " ", "CM", " ", 
       "frame", " ", "of", " ", "proton"}], "-", 
      RowBox[{
      "proton", " ", "collisions", " ", "the", " ", "meson", " ", "and", " ", 
       "ALP"}]}], " ", "|", "momenta", "|", " ", 
     RowBox[{"are", " ", "the", " ", "same"}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"plabMesonElabALP", "[", 
      RowBox[{"Ea_", ",", "mmeson_", ",", "ma_", ",", "Eplab_", ",", "MP_"}], 
      "]"}], "=", 
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{"Ea", ">", "0"}], ",", 
       RowBox[{"Simplify", "[", 
        RowBox[{"PlabPcm", "[", 
         RowBox[{
          RowBox[{"PcmPlab", "[", 
           RowBox[{
            SqrtBox[
             RowBox[{
              SuperscriptBox["Ea", "2"], "-", 
              SuperscriptBox["ma", "2"]}]], ",", "ma", ",", "Eplab", ",", 
            "MP"}], "]"}], ",", "mmeson", ",", "Eplab", ",", "MP"}], "]"}], 
        "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"ElabMesonElabALP", "[", 
      RowBox[{"Ea_", ",", "mmeson_", ",", "ma_", ",", "Eplab_", ",", "MP_"}], 
      "]"}], "=", 
     SqrtBox[
      RowBox[{
       SuperscriptBox[
        RowBox[{"plabMesonElabALP", "[", 
         RowBox[{"Ea", ",", "mmeson", ",", "ma", ",", "Eplab", ",", "MP"}], 
         "]"}], "2"], "+", 
       SuperscriptBox["mmeson", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Theta]labMeson\[Theta]ElabALP", "[", 
      RowBox[{
      "Ea_", ",", "\[Theta]a_", ",", "mmeson_", ",", "ma_", ",", "Eplab_", 
       ",", "MP_"}], "]"}], "=", 
     RowBox[{"ArcSin", "[", 
      RowBox[{
       RowBox[{"Sin", "[", "\[Theta]a", "]"}], 
       FractionBox[
        SqrtBox[
         RowBox[{
          SuperscriptBox["Ea", "2"], "-", 
          SuperscriptBox["ma", "2"]}]], 
        RowBox[{"plabMesonElabALP", "[", 
         RowBox[{"Ea", ",", "mmeson", ",", "ma", ",", "Eplab", ",", "MP"}], 
         "]"}]]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{"If", " ", 
