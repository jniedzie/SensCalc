Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "The", " ", "spectrum", " ", "of", " ", "kaons", " ", "is", " ", "taken", 
    " ", "from", " ", "2004.07974"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"KSPSdata", "=", 
     RowBox[{"Import", "[", 
      RowBox[{
       RowBox[{"FileNameJoin", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"NotebookDirectory", "[", "]"}], ",", 
          "\"\<spectra/SM particles/SPS/DoubleDistrK-SPS.dat\>\""}], "}"}], 
        "]"}], ",", "\"\<Table\>\""}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"EKSPSmin", ",", "EKSPSmax"}], "}"}], "=", 
     RowBox[{"MinMax", "[", 
      RowBox[{"KSPSdata", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "2"}], "]"}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Theta]KSPSmax", "=", 
     RowBox[{"Max", "[", 
      RowBox[{"KSPSdata", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DoubleDistrKatSPStemp", "[", 
      RowBox[{"\[Theta]K_", ",", "EK_"}], "]"}], "=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"EK", "<", "EKSPSmin"}], ",", "0", ",", 
       RowBox[{"Evaluate", "[", 
        RowBox[{"10", "^", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Interpolation", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"Log10", "[", 
                  RowBox[{"#", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
                 RowBox[{"Log10", "[", 
                  RowBox[{"#", "[", 
                   RowBox[{"[", "2", "]"}], "]"}], "]"}], ",", 
                 RowBox[{"Log10", "[", 
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "3", "]"}], "]"}], "+", 
                   SuperscriptBox["10", 
                    RowBox[{"-", "90"}]]}], "]"}]}], "}"}], "&"}], "/@", 
              "KSPSdata"}], ",", 
             RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}], "[", 
           RowBox[{
            RowBox[{"Log10", "[", "\[Theta]K", "]"}], ",", 
            RowBox[{"Log10", "[", "EK", "]"}]}], "]"}], ")"}]}], "]"}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Theta]listKSPS", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SuperscriptBox["10", 
         RowBox[{"-", "3"}]], ",", 
        RowBox[{"3", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "3"}]]}], ",", 
        RowBox[{"6", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "3"}]]}], ",", 
        SuperscriptBox["10", 
         RowBox[{"-", "2"}]], ",", 
        RowBox[{"2", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "2"}]]}], ",", 
        RowBox[{"4", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "2"}]]}], ",", 
        RowBox[{"6", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "2"}]]}], ",", 
        RowBox[{"8", "*", 
         SuperscriptBox["10", 
          RowBox[{"-", "2"}]]}], ",", 
        SuperscriptBox["10", 
         RowBox[{"-", "1"}]], ",", "0.12", ",", "0.15", ",", "0.25", ",", 
        "0.35", ",", "0.5", ",", "0.65", ",", "0.8", ",", "1.", ",", "1.2", 
        ",", "1.4", ",", "1.6", ",", "1.8", ",", "2.", ",", "2.2", ",", "2.4",
         ",", "2.6", ",", "2.8", ",", "3", ",", "3.1"}], "}"}], "//", "N"}]}],
     ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"emax\[Theta]K", "=", 
      RowBox[{"Quiet", "[", 
       RowBox[{"eMotherMax", "[", 
        RowBox[{
        "DoubleDistrKatSPStemp", ",", "mK", ",", "EKSPSmax", ",", 
         "\[Theta]listKSPS", ",", 
         RowBox[{"-", "5"}]}], "]"}], "]"}]}], ";"}], "//", 
    "AbsoluteTiming"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"EKmaxSPS", "[", "\[Theta]K_", "]"}], "=", 
      RowBox[{"10", "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Interpolation", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"Log10", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
               RowBox[{"Log10", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "}"}], "&"}], "/@", 
            "emax\[Theta]K"}], ",", 
           RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}], "[", 
         RowBox[{"Log10", "[", "\[Theta]K", "]"}], "]"}], ")"}]}]}], ";"}], "//",
     "AbsoluteTiming"}], "\[IndentingNewLine]", 
   RowBox[{"LogLogPlot", "[", 
    RowBox[{
     RowBox[{"EKmaxSPS", "[", "\[Theta]K", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]K", ",", 
       SuperscriptBox["10", 
        RowBox[{"-", "5"}]], ",", "\[Theta]KSPSmax"}], "}"}], ",", 
     RowBox[{"Frame", "\[Rule]", "True"}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
     RowBox[{"PlotRange", "\[Rule]", "All"}]}], "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DoubleDistrKSPS", "[", 
      RowBox[{"\[Theta]K_", ",", "EK_"}], "]"}], "=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox["10", 
          RowBox[{"-", "5."}]], "\[LessEqual]", "\[Theta]K", "\[LessEqual]", 
         "\[Theta]KSPSmax"}], "&&", 
        RowBox[{"mK", "<", "EK", "<", 
         RowBox[{"EKmaxSPS", "[", "\[Theta]K", "]"}]}]}], ",", 
       RowBox[{"Evaluate", "[", 
        RowBox[{"DoubleDistrKatSPStemp", "[", 
         RowBox[{"\[Theta]K", ",", "EK"}], "]"}], "]"}], ",", "0"}], "]"}]}], 
    ";"}]}]}]], "Input",
