       RowBox[{"[", "4", "]"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"EVminCM", "[", 
      RowBox[{"mmeson_", ",", "mV_"}], "]"}], "=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"mV", ">", "mmeson"}], ",", 
       RowBox[{"Evaluate", "[", 
        RowBox[{"EV", "/.", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"ECMmesonECMBL", "[", 
              RowBox[{"EV", ",", "mV", ",", "mmeson"}], "]"}], "\[Equal]", 
             "0"}], ",", "EV"}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], "]"}], ",", "mV"}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Jacobian", " ", 
      SubscriptBox["E", "meson"]}], ",", 
     RowBox[{
      SubscriptBox["\[Theta]", "meson"], " ", "\[Rule]", " ", 
      SubscriptBox["E", "a"]}], ",", 
     SubscriptBox["\[Theta]", "a"]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Jacobian\[Theta]mesonEmesonTo\[Theta]VEVBeamDump", "[", 
      RowBox[{
      "Eplab_", ",", "MP_", ",", "mmeson_", ",", "mV_", ",", "\[Theta]V_", 
       ",", "EV_"}], "]"}], "=", 
     RowBox[{
      RowBox[{"Abs", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"\[Theta]labMeson\[Theta]ElabBL", "[", 
            RowBox[{
            "EV", ",", "\[Theta]V", ",", "mmeson", ",", "mV", ",", "Eplab", 
             ",", "MP"}], "]"}], ",", "\[Theta]V"}], "]"}], "*", 
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"ElabMesonElabBL", "[", 
            RowBox[{"EV", ",", "mmeson", ",", "mV", ",", "Eplab", ",", "MP"}],
             "]"}], ",", "EV"}], "]"}]}], "-", 
